        "status": {
          "type": "array",
          "enum": [
            "live-propopsed-transfer-from-gb",
            "voluntary-arrangement",
            "voluntary-arrangement-receivership",
            "administration-order",
            "live-receiver-manager-on-at-least-one-charge",
            "administrative-receiver",
