          "items": {
            "$ref": "caseDates"
          }
        },
        "notes": {
          "type": "array",
          "description": "Notes that apply to the case.",
          "items": {
            "type": "string"
          }
        },
        "practitioners": {
          "type": "array",
          "description": "The practitioners for the case.",
          "items": {
            "$ref": "practitioners"
          }
        },
        "links": {
          "type": "array",
          "description": "The resources related to this case",
          "items": {
            "$ref": "links"
          }
