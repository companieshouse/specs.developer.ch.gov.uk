        "address": {
          "type": "array",
          "description": "The practitioners' address.",
          "items": {
            "$ref": "practitionerAddress"
          }
        },
        "appointed_on": {
          "type": "string",
          "description": "The date the practitioner was appointed on.",
          "format": "date"
        },
        "ceased_to_act_on": {
          "type": "string",
          "description": "The date the practitioner ceased to act for the case.",
          "format": "date"
        },
        "role": {
          "type": "string",
